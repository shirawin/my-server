@model IEnumerable<Repositories.GeneratedModels.Alarm>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Minhour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Maxhour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Minhour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Maxhour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Code)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Codealarm">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Codealarm">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Codealarm">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
